<?xml version="1.0" encoding="UTF-8"?>
<hlvm xmlns="http://hlvm.org/src/hlvm/Reader/XML/HLVM.rng" pubid="http://hlvm.org/src/hlvm/test/xml2xml/unaryarithmetic.hlx">
  <bundle id="unaryarithmetic">
    <program id="unaryarithmetic">
      <block>
        <autovar id="v1" type="bool">
          <true/>
        </autovar>
        <autovar id="v2" type="bool">
          <false/>
        </autovar>
        <not>
          <load>
            <ref id="v1"/>
          </load>
        </not>
        <and>
          <load>
            <ref id="v1"/>
          </load>
          <load>
            <ref id="v2"/>
          </load>
        </and>
        <or>
          <load>
            <ref id="v1"/>
          </load>
          <load>
            <ref id="v2"/>
          </load>
        </or>
        <nor>
          <load>
            <ref id="v1"/>
          </load>
          <load>
            <ref id="v2"/>
          </load>
        </nor>
        <xor>
          <load>
            <ref id="v1"/>
          </load>
          <load>
            <ref id="v2"/>
          </load>
        </xor>
        <eq>
          <load>
            <ref id="v1"/>
          </load>
          <load>
            <ref id="v2"/>
          </load>
        </eq>
        <ne>
          <load>
            <ref id="v1"/>
          </load>
          <load>
            <ref id="v2"/>
          </load>
        </ne>
        <gt>
          <load>
            <ref id="v1"/>
          </load>
          <load>
            <ref id="v2"/>
          </load>
        </gt>
        <lt>
          <load>
            <ref id="v1"/>
          </load>
          <load>
            <ref id="v2"/>
          </load>
        </lt>
        <ge>
          <load>
            <ref id="v1"/>
          </load>
          <load>
            <ref id="v2"/>
          </load>
        </ge>
        <le>
          <load>
            <ref id="v1"/>
          </load>
          <load>
            <ref id="v2"/>
          </load>
        </le>
      </block>
    </program>
  </bundle>
</hlvm>
